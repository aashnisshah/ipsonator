{
  "manifest_version": 2,

  "name": "Ipsonator",
  "description": "The Ipsonator brings only the best Lorem Ipsum variants to your screen!",
  "version": "1.0",

  "icons": {
    "128": "icon.png"
  },
  "author": "aashnisshah",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.whatthetrend.com/*",
    "http://api.whatthetrend.com/api/v2/trends.json",
    "http://api.whatthetrend.com/api/v2/trends/locations/top.json?place_type_code=12"
  ],
  "optional_permissions": ["tabs"]
}